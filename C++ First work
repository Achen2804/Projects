#include <iostream>

using namespace std;

int main()
{
    int m{};
    int n{};
    cout<<"Please enter the value of M: ";
    cin>>m;
    cout<<"Please enter the value of N: ";
    cin>>n;
    for(int x=0; x<m;x++){
        cout<<"\n";
        for(int y=0;y<n;y++){
            cout<<'*';
        }
    }

    return 0;
}
